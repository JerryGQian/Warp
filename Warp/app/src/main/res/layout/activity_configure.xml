<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ninja.qian.warp.Configure">

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="311dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="311dp"
        tools:layout_editor_absoluteX="0dp" />

    <RelativeLayout
        android:layout_width="356dp"
        android:layout_height="514dp"
        android:layout_marginTop="25dp"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="4dp"
        android:layout_centerHorizontal="true"
        tools:layout_editor_absoluteY="6dp">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/warplogoblue"
            android:layout_above="@+id/smooth_button"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:id="@+id/labelh1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/anchor_1"
            android:layout_alignBottom="@+id/anchor_1"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:onClick="onClick"
            android:text="Test" />

        <TextView
            android:id="@+id/labelh1N"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/labelh1"
            android:layout_alignRight="@+id/labelh1"
            android:layout_below="@+id/seekh1"
            android:onClick="onClick"
            android:text="Test" />

        <TextView
            android:id="@+id/labelh2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/anchor_2"
            android:layout_alignBottom="@+id/anchor_2"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:onClick="onClick"
            android:text="Test" />

        <TextView
            android:id="@+id/labelh2N"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/seekh2N"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:onClick="onClick"
            android:text="Test" />

        <Button
            android:id="@+id/smooth_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/stretch_button"
            android:layout_alignBottom="@+id/stretch_button"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:onClick="onClick"
            android:text="Synced" />

        <Button
            android:id="@+id/stretch_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text=" Stretch  "
            android:layout_alignBaseline="@+id/extend_button"
            android:layout_alignBottom="@+id/extend_button"
            android:layout_centerHorizontal="true" />

        <Button
            android:id="@+id/extend_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="89dp"
            android:text="Extend"
            android:onClick="onClick"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <SeekBar
            android:id="@+id/seekh1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/smooth_button"
            android:layout_marginTop="56dp"
            android:max="96" />

        <SeekBar
            android:id="@+id/seekh1N"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView3"
            android:max="96" />

        <SeekBar
            android:id="@+id/seekh2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/anchor_2"
            android:max="96" />

        <SeekBar
            android:id="@+id/seekh2N"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView5"
            android:max="96" />

        <TextView
            android:id="@+id/anchor_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/seekh1"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="23dp"
            android:text="Anchor 1: Original time" />

        <TextView
            android:id="@+id/anchor_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="27dp"
            android:text="Anchor 2: Original time"
            android:layout_below="@+id/seekh1N"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:id="@+id/syncbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="Match Anchor Pairs"
            android:layout_marginTop="7dp"
            android:layout_below="@+id/seekh2N"
            android:layout_centerHorizontal="true" />
        <Button
            android:id="@+id/exitbutton"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="Exit"
            android:layout_marginBottom="25dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/seekh1"
            android:text="Anchor 1: New Stretched time" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/seekh2"
            android:text="Anchor 2: New Stretched time" />




    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
