<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="ninja.qian.warp.Config$PlaceholderFragment">

    <TextView
        android:id="@+id/labelh1"
        android:onClick="onClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test"
        android:layout_marginRight="18dp"
        android:layout_marginEnd="18dp"
        android:layout_alignBaseline="@+id/anchor_1"
        android:layout_alignBottom="@+id/anchor_1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <TextView
        android:id="@+id/labelh1N"
        android:onClick="onClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test"
        android:layout_below="@+id/seekh1"
        android:layout_alignRight="@+id/labelh1"
        android:layout_alignEnd="@+id/labelh1" />
    <TextView
        android:id="@+id/labelh2"
        android:onClick="onClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test"
        android:layout_alignBaseline="@+id/anchor_2"
        android:layout_alignBottom="@+id/anchor_2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <TextView
        android:id="@+id/labelh2N"
        android:onClick="onClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test"
        android:layout_marginBottom="7dp"
        android:layout_above="@+id/seekh2N"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/smooth_button"
        android:onClick="onClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Smooth"
        android:layout_alignBaseline="@+id/stretch_button"
        android:layout_alignBottom="@+id/stretch_button"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:id="@+id/stretch_button"
        android:onClick="onClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stretch"
        android:layout_alignBaseline="@+id/extend_button"
        android:layout_alignBottom="@+id/extend_button"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/extend_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Extend"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="8dp" />

    <SeekBar
        android:id="@+id/seekh1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="54dp"
        android:layout_below="@+id/smooth_button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:max="96"/>

    <SeekBar
        android:id="@+id/seekh1N"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/anchor_1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="17dp"
        android:max="96"/>

    <SeekBar
        android:id="@+id/seekh2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:layout_above="@+id/seekh2N"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:max="96"/>

    <SeekBar
        android:id="@+id/seekh2N"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="64dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:max="96"/>

    <TextView
        android:id="@+id/anchor_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Anchor 1"
        android:layout_marginBottom="22dp"
        android:layout_alignBottom="@+id/seekh1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>

    <TextView
        android:id="@+id/anchor_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Anchor 2"
        android:layout_marginBottom="10dp"
        android:layout_above="@+id/seekh2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <!--<ExpandableListView
        android:id="@+id/expandableListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:indicatorLeft="?android:attr/expandableListPreferredItemIndicatorLeft"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="426dp"
        android:text="Mode"/>-->


</RelativeLayout>
